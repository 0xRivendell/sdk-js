    },
    {
      "path": "dist/intents.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.6",
    "husky": "^8.0.3",
    "size-limit": "^8.2.6",
    "ts-node": "^10.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6"
